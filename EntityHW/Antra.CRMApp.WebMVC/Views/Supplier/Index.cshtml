@model IEnumerable<SupplierModel>
<a asp-action="Create" asp-controller="Supplier" class="btn btn-success" >Add Supplier</a>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>CompanyName</th>
            <th>ContactName</th>
            <th>ContactTitle</th>
            <th>Address</th>
            <th>City</th>
            <th>RegionId</th>
            <th>PostalCode</th>
            <th>Country</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var m in Model)
        {
            <tr>
                <td>@m.Id</td>
                <td>@m.CompanyName</td>
                <td>@m.ContactName</td>
                <td>@m.ContactTitle</td>
                <td>@m.Address</td>
                <td>@m.City</td>
                <td>@m.RegionId</td>
                <td>@m.PostalCode</td>
                <td>@m.Country</td>
                <td>@m.Phone</td>
            </tr>
        }
    </tbody>
</table>
